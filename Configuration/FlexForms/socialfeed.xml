<T3DataStructure>
    <meta>
        <langDisable>1</langDisable>
    </meta>
    <sheets>
        <sDEF>
            <ROOT>
                <TCEforms>
                    <sheetTitle>LLL:EXT:pb_social/Resources/Private/Language/locallang_db.xlf:flexform.tab.general.caption</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <settings.refreshTimeInMin>
                        <TCEforms>
                            <label>how often does the feeds have to be updated ? (in min)</label>
                            <config>
                                <type>input</type>
                            </config>
                        </TCEforms>
                    </settings.refreshTimeInMin>
                    <settings.feedRequestLimit>
                        <TCEforms>
                            <label>request limit for posts (small amount => fast response ^^)</label>
                            <config>
                                <type>input</type>
                            </config>
                        </TCEforms>
                    </settings.feedRequestLimit>
                    <settings.onlyWithPicture>
                        <TCEforms>
                            <label>show only posts with pictures</label>
                            <config>
                                <type>check</type>
                            </config>
                        </TCEforms>
                    </settings.onlyWithPicture>
                    <settings.textTrimLength>
                        <TCEforms>
                            <label>trim text after reaching this letter limit (if empty => default 130)</label>
                            <config>
                                <type>input</type>
                            </config>
                        </TCEforms>
                    </settings.textTrimLength>
                </el>
            </ROOT>
        </sDEF>
        <display_facebook>
            <ROOT>
                <TCEforms>
                    <sheetTitle>LLL:EXT:pb_social/Resources/Private/Language/locallang_db.xlf:flexform.tab.facebook.caption</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <settings.facebookEnabled>
                        <TCEforms>
                            <label>LLL:EXT:pb_social/Resources/Private/Language/locallang_db.xlf:flexform.tab.facebook.active</label>
                            <!--<onChange>reload</onChange>-->
                            <config>
                                <type>check</type>
                            </config>
                        </TCEforms>
                    </settings.facebookEnabled>
                    <settings.facebookSearchIds>
                        <TCEforms>
                            <label>facebook search ID's</label>
                            <!--<displayCond>FIELD:settings.facebookEnabled:=:1</displayCond>-->
                            <config>
                                <type>input</type>
                            </config>
                        </TCEforms>
                    </settings.facebookSearchIds>
                </el>
            </ROOT>
        </display_facebook>
        <display_google>
            <ROOT>
                <TCEforms>
                    <sheetTitle>LLL:EXT:pb_social/Resources/Private/Language/locallang_db.xlf:flexform.tab.google.caption</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <settings.googleEnabled>
                        <TCEforms>
                            <label>LLL:EXT:pb_social/Resources/Private/Language/locallang_db.xlf:flexform.tab.google.active</label>
                            <!--<onChange>reload</onChange>-->
                            <config>
                                <type>check</type>
                            </config>
                        </TCEforms>
                    </settings.googleEnabled>
                    <settings.googleSearchIds>
                        <TCEforms>
                            <label>google search ID's</label>
                            <!--<displayCond>FIELD:settings.googleEnabled:=:1</displayCond>-->
                            <config>
                                <type>input</type>
                            </config>
                        </TCEforms>
                    </settings.googleSearchIds>
                </el>
            </ROOT>
        </display_google>
        <display_instagram>
            <ROOT>
                <TCEforms>
                    <sheetTitle>LLL:EXT:pb_social/Resources/Private/Language/locallang_db.xlf:flexform.tab.instagram.caption</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <settings.instagramEnabled>
                        <TCEforms>
                            <label>LLL:EXT:pb_social/Resources/Private/Language/locallang_db.xlf:flexform.tab.instagram.active</label>
                            <!--<onChange>reload</onChange>-->
                            <config>
                                <type>check</type>
                            </config>
                        </TCEforms>
                    </settings.instagramEnabled>
                    <settings.instagramSearchIds>
                        <TCEforms>
                            <label>instagram search ID's</label>
                            <!--<displayCond>FIELD:settings.instagramEnabled:=:1</displayCond>-->
                            <config>
                                <type>input</type>
                            </config>
                        </TCEforms>
                    </settings.instagramSearchIds>
                </el>
            </ROOT>
        </display_instagram>
        <display_twitter>
            <ROOT>
                <TCEforms>
                    <sheetTitle>LLL:EXT:pb_social/Resources/Private/Language/locallang_db.xlf:flexform.tab.twitter.caption</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <settings.twitterEnabled>
                        <TCEforms>
                            <label>LLL:EXT:pb_social/Resources/Private/Language/locallang_db.xlf:flexform.tab.twitter.active</label>
                            <!--<onChange>reload</onChange>-->
                            <config>
                                <type>check</type>
                            </config>
                        </TCEforms>
                    </settings.twitterEnabled>
                    <settings.twitterHideRetweets>
                        <TCEforms>
                            <label>LLL:EXT:pb_social/Resources/Private/Language/locallang_db.xlf:flexform.tab.twitter.HideRetweets</label>
                            <!--<onChange>reload</onChange>-->
                            <config>
                                <type>check</type>
                            </config>
                        </TCEforms>
                    </settings.twitterHideRetweets>
                    <settings.twitterSearchFieldValues>
                        <TCEforms>
                            <label>twitter search field values (e.g. #plusB,@plusB,...)</label>
                            <!--<displayCond>FIELD:settings.twitterEnabled:=:1</displayCond>-->
                            <config>
                                <type>input</type>
                            </config>
                        </TCEforms>
                    </settings.twitterSearchFieldValues>
                </el>
            </ROOT>
        </display_twitter>
        <display_tumblr>
            <ROOT>
                <TCEforms>
                    <sheetTitle>LLL:EXT:pb_social/Resources/Private/Language/locallang_db.xlf:flexform.tab.tumblr.caption</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <settings.tumblrEnabled>
                        <TCEforms>
                            <label>LLL:EXT:pb_social/Resources/Private/Language/locallang_db.xlf:flexform.tab.tumblr.active</label>
                            <!--<onChange>reload</onChange>-->
                            <config>
                                <type>check</type>
                            </config>
                        </TCEforms>
                    </settings.tumblrEnabled>
                    <settings.tumblrBlogNames>
                        <TCEforms>
                            <label>tumblr blog names ... for own Dashboard use "MYDASHBOARD" as value</label>
                            <!--<displayCond>FIELD:settings.tumblrEnabled:=:1</displayCond>-->
                            <config>
                                <type>input</type>
                            </config>
                        </TCEforms>
                    </settings.tumblrBlogNames>
                </el>
            </ROOT>
        </display_tumblr>
    </sheets>
</T3DataStructure>